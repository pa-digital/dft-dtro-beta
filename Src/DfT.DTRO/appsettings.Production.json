{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.GoogleCloudLogging",
      "Serilog.Expressions",
      "Serilog.Settings.Configuration"
    ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "(StartsWith(SourceContext, 'Microsoft') or StartsWith(SourceContext, 'System')) and (@l = 'Information' or @l = 'Debug' or @l = 'Verbose')"
        }
      }
    ],
    "WriteTo": [
      {
        "Name": "GoogleCloudLogging",
        "Args": {
          "projectID": "",
          "serviceName": "DTRO_Api",
          "serviceVersion": "beta"
        }
      }
    ]
  },
  "FeatureManagement": {
    "DtroRead": true,
    "DtroWrite": true,
    "SchemasRead": true,
    "SchemaWrite": true
  },
  "RequestCorrelation": {
    "HeaderName": "X-Correlation-ID",
    "LogPropertyName": "correlationId"
  }
}
