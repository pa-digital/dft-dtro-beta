{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.GoogleCloudLogging"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "GoogleCloudLogging",
        "Args": {
          "projectID":  "",
          "serviceName": "DTRO_Api",
          "serviceVersion": "beta"
        }
      }
    ]
  },
  "FeatureManagement": {
    "DtroRead": true,
    "DtroWrite": true,
    "SchemasRead": true,
    "SchemaWrite": true
  },
  "RequestCorrelation": {
    "HeaderName": "X-Correlation-ID",
    "LogPropertyName": "correlationId"
  }
}
